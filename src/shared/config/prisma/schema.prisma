generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}
 
model User {
  id            String          @id @default(cuid())
  companyName          String
  email         String          @unique
  phoneNumber  String
  passwordHash String
  imageUrl     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
 


 
